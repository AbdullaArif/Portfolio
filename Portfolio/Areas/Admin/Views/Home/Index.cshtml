@model List<Contact>
<div class="content-wrapper">
    <div class="row">

        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Your Message</h4>
                   
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>
                                       Full Name
                                    </th>
                                    <th>
                                        Email
                                    </th>
                                    <th>
                                        Phone
                                    </th>
                                    <th>
                                        Message
                                    </th>
                                    <th>
                                        Settings
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var contact in Model)
                                {
                                 <tr>
                                    <td class="py-1">
                                        @contact.FullName
                                    </td>
                                    <td>
                                       @contact.Email
                                    </td>
                                    <td>
                                        <div >
                                            @contact.Phone
                                        </div>
                                    </td>
                                    <td>
                                       @contact.Message
                                    </td>
                                    <td>
                                        <button  class="btn btn-danger" asp-area="admin" asp-controller="home" asp-action="delete">Delete</button>
                                    </td>
                                </tr>   
                                }
                                
s
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
